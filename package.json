  "scripts": {
    "dev": "next dev --experimental-https",
    "build": "next build",
    "start": "next start",
    "postinstall": "pnpm prisma generate",
    "db:generate": "pnpm prisma generate",
    "db:migrate": "pnpm prisma migrate dev",
    "db:push": "pnpm prisma db push",
    "db:pull": "npx prisma db pull",
    "db:reset": "npx prisma migrate reset",
    "db:studio": "npx prisma studio",
    "shadcn:add": "pnpm dlx shadcn-ui@latest add",
    "prettier:format": "prettier --write \"**/*.{ts,tsx,mdx}\" --cache",
    "prettier:check": "prettier --check \"**/*.{ts,tsx,mdx}\" --cache",
    "packages:update-all": "pnpm up --latest",
    "typecheck": "tsc --noEmit",
    "lint": "next lint",
    "lint:fix": "next lint --fix"
